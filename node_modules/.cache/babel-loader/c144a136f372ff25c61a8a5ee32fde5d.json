   <form @submit.prevent=\"handleSubmit\">\r\n        <label>Email:</label>\r\n        <input type=\"email\" required v-model=\"email\">\r\n        <label>Password:</label>\r\n        <input type=\"password\" required v-model=\"password\">\r\n        <div v-if=\"passwordError\" class=\"error\">{{ passwordError }}</div>\r\n\r\n        <label>Role:</label>\r\n        <!--v-model=\"role\"表示<select>元素的選中值將與Vue實例的role屬性進行雙向綁定，當用戶從下拉選單中選擇一個選項時，role屬性的值會更新為對應選項的value值-->\r\n        <select v-model=\"role\">\r\n            <option value=\"developer\">developer</option>\r\n            <option value=\"designer\">designer</option>\r\n        </select>\r\n        <label>Skills (press alt + comma to add):</label>\r\n        <input type=\"text\" v-model=\"tempSkill\" @keyup.alt=\"addSkill\">\r\n        <div v-for=\"skill in skills\" :key=\"skill\" class=\"pill\">\r\n            <span @click=\"deleteSkill(skill)\">{{ skill }}</span>\r\n        </div>\r\n\r\n        <div class=\"terms\">\r\n            <input type=\"checkbox\" required v-model=\"terms\">\r\n            <label>我同意使用者隱私權政策和使用條款</label>\r\n        </div>\r\n        <div class=\"submit\">\r\n            <button>Create an Account</button>\r\n        </div>\r\n    </form>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            email: \"\",\r\n            password: \"\",\r\n            role: 'developer',\r\n            terms: false,\r\n            tempSkill: \"\",\r\n            skills: [],\r\n            passwordError: \"\",\r\n        }\r\n    },\r\n    methods: {\r\n        addSkill($event) {\r\n            if ($event.key === ',' && this.tempSkill) {\r\n                if (!this.skills.includes(this.tempSkill)) {\r\n                    this.skills.push(this.tempSkill)\r\n                }\r\n                this.tempSkill = ''\r\n            }\r\n        },\r\n        deleteSkill(skill) {\r\n            // 從技能列表中移除指定的技能\r\n            this.skills = this.skills.filter((item) => {\r\n                // 只保留不等於要刪除的技能的元素\r\n                return skill != item;\r\n            });\r\n        },\r\n        handleSubmit() {\r\n            // validate password\r\n            this.passwordError = this.password.length > 5 ?\r\n                \"\" : \"Password must be at least 6 chars long.\";\r\n            console.log(\"Form submitted\");\r\n            if (!this.passwordError) {\r\n                console.log('email: ', this.email)\r\n                console.log('password: ', this.password)\r\n                console.log('role: ', this.role)\r\n                console.log('skills: ', this.skills)\r\n                console.log('terms accepted: ', this.terms)\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\nform {\r\n    max-width: 420px;\r\n    margin: 30px auto;\r\n    background: white;\r\n    text-align: left;\r\n    padding: 40px;\r\n    border-radius: 10px;\r\n}\r\n\r\nlabel {\r\n    color: #aaa;\r\n    display: inline-block;\r\n    margin: 25px 0 15px;\r\n    font-size: 0.6em;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    font-weight: bold;\r\n}\r\n\r\ninput,\r\nselect {\r\n    display: block;\r\n    padding: 10px 6px;\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n    border: none;\r\n    border-bottom: 1px solid #ddd;\r\n    color: #555;\r\n}\r\n\r\ninput[type=\"checkbox\"] {\r\n    display: inline-block;\r\n    width: 16px;\r\n    margin: 0 10px 0 0;\r\n    position: relative;\r\n    top: 2px;\r\n}\r\n\r\n.pill {\r\n    display: inline-block;\r\n    margin: 20px 10px 0 0;\r\n    padding: 6px 12px;\r\n    background: #eee;\r\n    border-radius: 20px;\r\n    font-size: 12px;\r\n    letter-spacing: 1px;\r\n    font-weight: bold;\r\n    color: #777;\r\n    cursor: pointer;\r\n}\r\n\r\nbutton {\r\n    background: #0b6dff;\r\n    border: 0;\r\n    padding: 10px 20px;\r\n    margin-top: 20px;\r\n    color: white;\r\n    border-radius: 20px;\r\n    cursor: pointer;\r\n}\r\n\r\n.submit {\r\n    text-align: center;\r\n}\r\n\r\n.error {\r\n    color: #ff0062;\r\n    margin-top: 10px;\r\n    font-size: 0.8em;\r\n    font-weight: bold;\r\n}\r\n\r\n.info-preview {\r\n    margin-top: 20px;\r\n    padding: 20px;\r\n    background-color: #f9f9f9;\r\n    border: 1px solid #eaeaea;\r\n    border-radius: 5px;\r\n}\r\n\r\n.info-preview p {\r\n    color: #333;\r\n    font-size: 16px;\r\n    margin: 10px 0;\r\n}\r\n</style>"],"mappings":";AAwCA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,WAAW;MACjBC,KAAK,EAAE,KAAK;MACZC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE;IACnB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,QAAQA,CAACC,MAAM,EAAE;MACb,IAAIA,MAAM,CAACC,GAAE,KAAM,GAAE,IAAK,IAAI,CAACN,SAAS,EAAE;QACtC,IAAI,CAAC,IAAI,CAACC,MAAM,CAACM,QAAQ,CAAC,IAAI,CAACP,SAAS,CAAC,EAAE;UACvC,IAAI,CAACC,MAAM,CAACO,IAAI,CAAC,IAAI,CAACR,SAAS;QACnC;QACA,IAAI,CAACA,SAAQ,GAAI,EAAC;MACtB;IACJ,CAAC;IACDS,WAAWA,CAACC,KAAK,EAAE;MACf;MACA,IAAI,CAACT,MAAK,GAAI,IAAI,CAACA,MAAM,CAACU,MAAM,CAAEC,IAAI,IAAK;QACvC;QACA,OAAOF,KAAI,IAAKE,IAAI;MACxB,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAAA,EAAG;MACX;MACA,IAAI,CAACX,aAAY,GAAI,IAAI,CAACL,QAAQ,CAACiB,MAAK,GAAI,IACxC,EAAC,GAAI,yCAAyC;MAClDC,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;MAC7B,IAAI,CAAC,IAAI,CAACd,aAAa,EAAE;QACrBa,OAAO,CAACC,GAAG,CAAC,SAAS,EAAE,IAAI,CAACpB,KAAK;QACjCmB,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAACnB,QAAQ;QACvCkB,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE,IAAI,CAAClB,IAAI;QAC/BiB,OAAO,CAACC,GAAG,CAAC,UAAU,EAAE,IAAI,CAACf,MAAM;QACnCc,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACjB,KAAK;MAC9C;IACJ;EACJ;AACJ"}]}